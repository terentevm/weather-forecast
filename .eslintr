{
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "mocha": true
  },
  "extends": "airbnb",
  "rules": {
    "func-names": ["error", "never"],
//    "indent": ["warn", 4],
//    "react/jsx-indent" : ["warn", 4],

    "linebreak-style": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/no-extraneous-dependencies": ["warn", {"devDependencies": true}]
  }
}